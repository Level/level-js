{
  "name": "level-js",
  "version": "6.1.0",
  "description": "An abstract-leveldown compliant store on top of IndexedDB",
  "author": "max ogden",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "standard && hallmark && airtap -p local --coverage test/index.js",
    "test-browsers": "standard && airtap --coverage test/index.js",
    "coverage": "nyc report -r lcovonly",
    "hallmark": "hallmark --fix",
    "dependency-check": "dependency-check --no-dev -i buffer .",
    "prepublishOnly": "npm run dependency-check"
  },
  "files": [
    "index.js",
    "iterator.js",
    "util",
    "CHANGELOG.md",
    "UPGRADING.md",
    "sauce-labs.svg"
  ],
  "dependencies": {
    "abstract-leveldown": "^7.2.0",
    "buffer": "^6.0.3",
    "inherits": "^2.0.3",
    "ltgt": "^2.1.2",
    "run-parallel-limit": "^1.1.0"
  },
  "devDependencies": {
    "airtap": "^4.0.1",
    "airtap-playwright": "^1.0.1",
    "airtap-sauce": "^1.1.0",
    "dependency-check": "^4.1.0",
    "hallmark": "^3.0.0",
    "level-concat-iterator": "^3.0.0",
    "nyc": "^15.0.0",
    "standard": "^16.0.3",
    "tape": "^5.0.0",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level/level-js.git"
  },
  "homepage": "https://github.com/Level/level-js",
  "keywords": [
    "level",
    "leveldb",
    "indexeddb",
    "abstract-leveldown"
  ]
}
